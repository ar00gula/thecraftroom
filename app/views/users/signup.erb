<h1>Sign Up</h1>

<%= form_with model: @user, local: true do |f| %>
<%= render partial: "users/form", locals: {user: @user} %>
   <div class="field">
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>
  </div>

  <div class="actions">
    <%= f.submit "Sign Up" %>
  </div>
<% end %>

<%= link_to 'Back', users_path %>
